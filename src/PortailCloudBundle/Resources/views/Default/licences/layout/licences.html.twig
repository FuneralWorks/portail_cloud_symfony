{% extends "::base.html.twig" %} 
{% block javascript %}
{% endblock javascript %}
{% block breadcumb %}
    <li class="breadcrumb-item"><a href="{{ path('abonnements') }}">Abonnements</a></li>
    <li class="breadcrumb-item"><a href="{{ path('produits', { 'abonnementId' : abonnementId }) }}">Abonnement {{ abonnementId }}</a></li>
    <li class="breadcrumb-item active"><a href="">Produit {{ produitId }}</a></li>

{% endblock breadcumb %}
{% block body %}

<div class="animated fadeIn">
    <div class="row">
        <div class="col-lg-12">
            <div class="card">
                <div class="card-header">
                    Licences
                </div>
                <div class="card-block">
                    <table class="table">
                        <thead>
                            <tr>
                                <th>Nom Licence</th>
                                <th>SKU</th>
                                <th>Quantité</th>
                                <th>Désignation</th>
                                
                            </tr>
                        </thead>
                        <tbody>
                            {% for i in 0..10 %}
                            <tr>
                                <td><a class="nav-link" href="{{ path('dashboard') }}">Licences {{ i }}</a></td>
                                <td>RAF3443</td>
                                <td>5</td>
                                <td>Interessant</td>
                                
                            </tr>
                            {% endfor %}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

{% endblock body %}